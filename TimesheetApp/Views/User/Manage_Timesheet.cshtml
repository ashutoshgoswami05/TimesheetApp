@model  IEnumerable<TimesheetApp.Models.ManageViewModel>
@{
    ViewBag.Title = "Manage Timesheet";
}

    <div style="display:none" class="alert alert-success" id="ApproveAlert" role="alert">
        <strong>Success! </strong>Approved.
        <button type="button" class="close" data-hide="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>

    <div style="display:none" class="alert alert-info" id="RejectAlert" role="alert">
        <strong>Rejected! </strong>Timesheet Rejected.
        <button type="button" class="close" data-hide="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<div id="DisplayMemberTimesheets">
    @{ Html.RenderPartial("_MemberTimesheets",Model);}
</div>